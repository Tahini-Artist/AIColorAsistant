{
  "buildCommand": "npm run build",
  "outputDirectory": "template/app/dist",
  "framework": "vite",
  "installCommand": "cd template/app && npm install",
  "prebuildCommand": "npm run prebuild",
  "rewrites": [
    {
      "source": "/color-block/:colorId.png",
      "destination": "/api/color-block.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
