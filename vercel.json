{
  "buildCommand": "cd template/app && npm run generate-color-blocks && npm run build",
  "outputDirectory": "template/app/dist",
  "framework": "vite",
  "installCommand": "cd template/app && npm install",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
