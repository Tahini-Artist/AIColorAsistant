{
  "name": "wasp-repo",
  "version": "0.1.0",
  "description": "AI Color Assistant - Wasp-based SaaS application",
  "private": true,
  "scripts": {
    "prettier:check": "prettier --ignore-unknown --check --config .prettierrc .",
    "prettier:format": "prettier --ignore-unknown --write --config .prettierrc .",
    "dev": "cd template/app && npm run dev",
    "build": "cd template/app && npm run build",
    "preview": "cd template/app && npm run preview"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Tahini-Artist/AIColorAsistant.git"
  },
  "license": "MIT",
  "devDependencies": {
    "prettier": "3.5.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-pkg": "0.19.1",
    "prettier-plugin-sh": "0.17.4",
    "prettier-plugin-tailwindcss": "0.6.13",
    "typescript": "~5.8.3"
  }
}
